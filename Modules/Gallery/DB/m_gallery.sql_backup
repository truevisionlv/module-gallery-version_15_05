CREATE TABLE `m_gallery` (  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,  `image` varchar(255) NOT NULL,  `active` tinyint(1) unsigned NOT NULL DEFAULT '0',  `order` int(10) unsigned NOT NULL,  `title` int(10) unsigned NOT NULL COMMENT 'translation',  `category_id` int(10) unsigned NOT NULL,  PRIMARY KEY (`id`)) ENGINE=MyISAM DEFAULT CHARSET=utf8